[
    {
        "match": "ask(?X.goal(X))",
        "utterance": "Do you want to call someone or know someone's phone number?|What would you like to do?"
    },
    {
        "match": "icm:loadplan",
        "utterance": "Let's see..."
    },
    {
        "match": "['icm:acc*pos', 'icm:loadplan', 'ask(?X.goal(X))']",
        "utterance": "Do you want to call someone or know someone's phone number?"
    },
    {
        "match": "ask(?X.phone_type(X))",
        "utterance": "Which one of @selected_contact's phones?"
    },
    {
        "match": "ask(?X.selected_contact(X))",
        "utterance": "Which contact?"
    },
    {
        "match": "report(ServiceResultProposition(Call, [selected_contact(&individual), phone_type(&individual)], SuccessfulServiceAction()))",
        "utterance": "Calling @selected_contact's @phone_type.|I am calling @selected_contact."
    },
    {
        "match": "answer(phone_number(&individual))",
        "utterance": "The phone number is &individual."
    }
]